{% extends "layout.html.twig" %}

{% block head %}{% endblock %}
{% block title %}{% endblock %}
{% block content %}
    <ul>
        {% for post in posts %}
            <li> {{ post.id }}{{ post.title }} {{ post.content }}</li>
        {% else %}
            <li><em>no posts found</em></li>
        {% endfor %}
    </ul>
{% endblock %}
